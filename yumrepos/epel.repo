[epel-source]
name=Extra Packages for Enterprise Linux 7 - $basearch - Source
baseurl=https://dl.fedoraproject.org/pub/epel/7/SRPMS
enabled=0
gpgkey=https://dl.fedoraproject.org/pub/epel/RPM-GPG-KEY-EPEL-7
gpgcheck=1
